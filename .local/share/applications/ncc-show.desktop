[Desktop Entry]
Name=Netcat Chat - Show
Name[en_GB]=Netcat Chat - Show
Comment=Toggle the display of the Netcat Chat window
Type=Application
Path=
Exec=bash -c 'xdotool windowactivate "$(printf '%d' "$(wmctrl -l | grep -E "Netcat Chat Client" | cut -d " " -f 1)")"'
Icon=ncc
Terminal=false
StartupNotify=false

